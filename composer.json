{
    "name": "senhormrtroll/azureservicebus",
    "description": "Este é um fork do SDK PHP Azure para o barramento de serviços",
    "keywords": [ "php", "azure", "sdk" ],
    "license": "Apache-2.0",
    "type": "package",
    "authors": [
        {
            "name": "Azure Service Bus SDK PHP",
            "email": "senhormrtroll@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "pear/net_url2": "^2.2",
        "pear/mail_mime": "^1.10",
        "firebase/php-jwt": "^4.0 || ^5.0",
        "guzzlehttp/guzzle": "^7.0",
        "zendframework/zend-mime": "^2.6",
        "zendframework/zend-mail": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35",
        "phpdocumentor/phpdocumentor": "^2.8"
    },
    "autoload": {
        "psr-4": { "AzureServiceBus\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    }
}
